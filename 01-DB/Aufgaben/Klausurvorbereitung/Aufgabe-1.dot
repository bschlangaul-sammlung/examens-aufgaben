graph ER {
  edge[len=1.6]
  bgcolor=transparent;

  // Entity-Types

  node[shape=box]; Fluggesellschaft;
  node[shape=ellipse]; Name[label=<<u>Name</u>>]; Hauptsitz;
  Fluggesellschaft--Name;
  Fluggesellschaft--Hauptsitz;

  node[shape=box]; Flugzeug;
  node[shape=ellipse]; Flugzeugnummer[label=<<u>Flugzeugnummer</u>>]; Typ; Sitzplatzanzahl; Besatzung;
  Flugzeug--Flugzeugnummer;
  Flugzeug--Typ;
  Flugzeug--Sitzplatzanzahl;
  Flugzeug--Besatzung;

  node[shape=box]; Flug;
  node[shape=ellipse]; Flugnummer[label=<<u>Flugnummer</u>>]; Abflugzeit; Zielflughafen;
  Flug--Flugnummer;
  Flug--Abflugzeit;
  Flug--Zielflughafen;

  node[shape=box]; Passagier;
  node[shape=ellipse];
    PassagierNummer[label=<<u>PassagierNummer</u>>];
    PassagierName;
    Geburtsdatum;
  Passagier--PassagierNummer;
  Passagier--PassagierName;
  Passagier--Geburtsdatum;

  // Relationship-Types

  node[shape=diamond] buchen;
  node[shape=ellipse] Sitzplatznummer;
  buchen--Sitzplatznummer;
  // Entities
  buchen--Passagier[label=n];
  buchen--Flug[label=m];

  node[shape=diamond] besitzen;
  node[shape=ellipse] Anschaffungsjahr;
  besitzen--Anschaffungsjahr;
  // Entities
  besitzen--Fluggesellschaft[label=1];
  besitzen--Flugzeug[label=n];

  node[shape=diamond] fliegen;
  // Entities
  fliegen--Fluggesellschaft[label=1];
  fliegen--Flugzeug[label=1];
  fliegen--Flug[label=1];
}
