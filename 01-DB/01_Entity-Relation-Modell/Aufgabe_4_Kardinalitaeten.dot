graph ER {
	bgcolor=transparent;
    len=1.4

	node [shape=box]; Fahrer; Fahrzeug; Abteilung; Garage;
	node [shape=diamond]; hat_Fahrerlaubnis; steht_in; gehoert;

	gehoert -- Fahrzeug [label="(0,1)"];
	gehoert -- Abteilung [label="(1,*)"];

	steht_in -- Garage  [label="(1,1)"];
	steht_in -- Fahrzeug [label="(0,1)"];

	hat_Fahrerlaubnis -- Fahrer [len=2,label="(0,*)"];
	hat_Fahrerlaubnis -- Fahrzeug [len=2,label="(3,*)"];
}
