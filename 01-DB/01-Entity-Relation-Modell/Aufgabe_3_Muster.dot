graph ER {
	edge [len=1.8];
	bgcolor="transparent";

	node [shape=box]; Bauteil;
	node [shape=ellipse]; Nummer[label=<<u>Nummer</u>>]; BauteilBezeichnung; Hersteller; Einkaufspreis; Vorrat;

	Bauteil -- Nummer;
	Bauteil -- BauteilBezeichnung;
	Bauteil -- Hersteller;
	Bauteil -- Einkaufspreis;
	Bauteil -- Vorrat;

	node [shape=box]; Geraet;
	node [shape=ellipse]; GeraetBezeichnung[label=<<u>GeraetBezeichnung</u>>]; Lagerbestand; Verkaufspreis; Anliefergebuehr;
	Geraet -- GeraetBezeichnung;
	Geraet -- Lagerbestand;
	Geraet -- Verkaufspreis;
	Geraet -- Anliefergebuehr;

	node [shape=box]; Kunde;
	node [shape=ellipse]; KdNr[label=<<u>KdNr</u>>]; Name; Adresse; Branche;
	Kunde -- KdNr;
	Kunde -- Name;
	Kunde -- Adresse;
	Kunde -- Branche;

	node [shape=box]; Kundenbetreuer;
	node [shape=ellipse]; BetreuerName;
	Kundenbetreuer -- BetreuerName;

	node [shape=box]; Bezirk;
	node [shape=ellipse]; BezirkNummer[label=<<u>BezirkNummer</u>>]; Beschreibung;
	Bezirk -- BezirkNummer;
	Bezirk -- Beschreibung;

	node [shape=diamond]; gehoert_zu;
	gehoert_zu -- Bezirk [label=1];
	gehoert_zu -- Kunde [label=1];

	node [shape=diamond]; betreut;
	betreut -- Kundenbetreuer [label=1];
	betreut -- Bezirk [label=1];

	node [shape=diamond]; besteht_aus;
	besteht_aus -- Geraet [label=n];
	besteht_aus -- Bauteil [label=m];

	node [shape=diamond]; wird_beliefert_an;
	node [shape=ellipse]; Sonderkonditionen;
	wird_beliefert_an -- Sonderkonditionen;
	wird_beliefert_an -- Kunde [label=n];
	wird_beliefert_an -- Geraet [label=m];
}
