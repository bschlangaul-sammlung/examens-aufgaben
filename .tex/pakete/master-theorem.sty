% \begin{macro}{\liRundeKlammer}
%    \begin{macrocode}
\def\liRundeKlammer#1{
  \negthinspace \left( #1 \right)
}
%    \end{macrocode}
% \end{macro}

% \begin{macro}{\liTheta}
% |\liTheta{n^2}|: \liTheta{n^2}
%    \begin{macrocode}
\def\liThetaOhneMathe#1{
  \Theta \liRundeKlammer{#1}
}
\def\liTheta#1{
  \ifmmode
    \liThetaOhneMathe{#1}
  \else
    $\liThetaOhneMathe{#1}$
  \fi
}
%    \end{macrocode}
% \end{macro}

