\NeedsTeXFormat{LaTeX2e}[1995/12/01]%
\ProvidesPackage{tikz-uml-activity}[2020/07/20]%
\RequirePackage{tikz}
\usetikzlibrary{calc}
\usetikzlibrary{shapes.geometric}
\usetikzlibrary{arrows.meta,arrows}
\tikzset{
  uml activity/.style={
    initial/.style={
      circle,
      fill
    },
    decision/.style={
      diamond,
      black,
      draw
    },
    action/.style={
      rectangle,
      draw,
      rounded corners=8pt,
      text width=2.5cm,
      minimum height=1cm,
      align=center,
    },
    object/.style={
      rectangle,
      draw,
    },
    arrow/.style={
      draw,
      -{Straight Barb[length=4pt]}
    },
    arrow to/.style={
      arrow,
    },
    arrow from/.style={
      draw,
      {Straight Barb[length=4pt]}-
    },
    flow final/.style={
      circle,
      draw,
      fill,
      double,
      double distance=1pt,
    },
    activity final/.style={
      circle,
      draw,
      append after command={
        \pgfextra{
          \draw (\tikzlastnode.north east) -- (\tikzlastnode.south west);
          \draw (\tikzlastnode.north west) -- (\tikzlastnode.south east);
        }
      },
    }
  }
}
