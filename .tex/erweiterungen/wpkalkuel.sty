\RequirePackage{amsmath}
\def\wp#1#2{
  \text{wp}(\texttt{\scriptsize"#1"},\thinspace #2)
}

\def\MatheEnv#1{
  \medskip

  \hspace{1em}#1

  \medskip
}

\def\Mathe#1{
  \MatheEnv{$#1$}
}

\def\MatheEquiv#1{
  \MatheEnv{$\equiv$\hspace{1em}$#1$}
}
